float mefade = 1.0 - clamp(abs(timeAngle - 0.5) * 8.0 - 1.5, 0.0, 1.0);
float dfade = 1.0 - timeBrightness;

vec3 calcLightColor(vec3 morning, vec3 day, vec3 afternoon, vec3 night, vec3 weather){
	vec3 c = mix(night, mix(mix(morning, afternoon, mefade), day, 1.0 - dfade * sqrt(dfade)), sunVisibility);
	c = mix(c, dot(c, vec3(0.299, 0.587, 0.114)) * weather, rainStrength);
	return c * c;
}

